Question 1.5:

const Change = /\b1[01]{7}\b/g;
const input = "11010001 00011100 01000111 10100100 01101101 10101110 10000110 11100101 00101101 00011010 11111010 11011011 10110110 01111101 11011000 01110000 10101100 00000110 01010110 11011000 01000110 01011101 11101011 11010101 11010000 10000000 11001001 10101100 10000110 00100101 11001111 01001101 11011110 10001011 11001000 00011010 01011011 10110011 01110111 11110000 11000010 11010001 10110010 10000000 10011100 01110101 10111101 11010010 11110111 10100010 00110011 10011010 10110011 01101101 11010100 01001000 01100011 01011111 10001000 00111110 00110101 00010010 10101110 01000001 01111111 10110000 11000100 11110011 10010111 00100101 10011011 11111111 00011000 11000110 11000000 10001011 01000100 00000001 10101110 01101010 11010010 10100101 00000011 01000110 10110111 10000000 00100110 11110011 00010000 11101111 11011111 10000000 00000110 10001100 10110100 00011000 00100100 00101000 11001111 01111110";

const output = input.replace(Change, function(number) {
  const rightMost = number.lastIndexOf('1'); //find last 1 (least significant)
  let result = '';

  for (let i = 0; i < number.length; i++) { //loop through each byte
    if (i < rightMost) { //for numbers before the rightmost 1
      if (number[i] === '0') { // if 0, swap to 1
        result += '1';
      } else if (number[i] === '1') { // if 1, swap to 0
        result += '0';
      }
    } else {
      result += number[i];
    }
  }

  //if super rare case of the rightmost 1 being the first number
  if (result[0] === '1') {
    result = result.replace(/^1/, '0');
  }

  return result;
});

console.log(output);

